<div class='container'>
    <div class='jumbotron text-center'>
        <h1>Workout Lounge</h1>
        <p>Show off your workout to your cat</p>
    </div>
    <div class='row'>
        <div class='col-md-8 col-md-offset-2'>
            <h2>Members</h2>
        </div>
    </div>
    <div class='row'>
        <% @athletes.each do |athlete| %>
            <div class='col-md-8 col-md-offset-2 well member'>
                <%= athlete.full_name %>
            </div>
        <% end %>
    </div>
</div>